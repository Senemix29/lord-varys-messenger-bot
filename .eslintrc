{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "valid-jsdoc": 2,
    "no-var": 0,
    "max-len": 0,
    "consistent-return": 0,
    "arrow-body-style": 0,
    "react/jsx-no-bind": 0,
    "no-param-reassign": [2, { "props": false }],
    "no-console": 0,
    "no-script-url": 0,
    "strict": 0,
  }
}
